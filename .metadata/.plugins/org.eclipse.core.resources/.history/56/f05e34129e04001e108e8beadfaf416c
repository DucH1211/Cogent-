		}
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("Write something:");
		String s = sc.nextLine();
		System.out.println(reverseString(s));
		
		
	}

}
