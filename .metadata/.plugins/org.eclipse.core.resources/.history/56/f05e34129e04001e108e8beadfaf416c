package cogent.edu.corejava.String;
import java.util.Scanner;

public class Reverse {

	public static String reverseString(String s)
	{	
		String str ="";
		char c = 0;
		for(int i = 0; i < s.length(); i ++)
		{
			c = s.charAt(i);
			str = c + str;
		}
		return str;
	}
	public static boolean Palyndrome(String s) {
