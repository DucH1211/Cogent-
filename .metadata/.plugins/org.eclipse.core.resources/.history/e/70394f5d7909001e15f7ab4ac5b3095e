package cogent.edu.corejava.Inheritance;

import java.io.Serializable;
import java.util.HashMap;

public class Employee implements Comparable,Serializable {
	private static final long serialVersionUID = 10L;
	int empid;
	String Name;
	int age;
	String Gender;
	int salary;	
	public Employee() {
		super();
	}


	public Employee(int empid, String name, int age, String gender,int salary) {
		super();
		this.empid = empid;
		Name = name;
		this.age = age;
		Gender = gender;
		this.salary = salary;
	}
	@Override
	public int compareTo(Object o) {
		// TODO Auto-generated method stub
		return 0;
	}
	@Override
	public String toString() {
		return "\n[id: " + empid + " Name: " + Name + " age: "+ age + " Gender: "+ Gender + " Salary: " + salary +"]\n";
	}

}
